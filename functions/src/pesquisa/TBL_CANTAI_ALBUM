

SELECT A.* FROM cantai_sh.TBL_CANTAI_ALBUM A
INNER JOIN TBL_CANTAI_MUSICA_ALBUM B ON A.ID_ALBUM = B.CODG_ALBUM
INNER JOIN TBL_CANTAI_MUSICA C ON B.CODG_MUSICA = C.ID_MUSICA

INNER JOIN TBL_CANTAI_CATEGORIA_MUSICA D ON C.ID_MUSICA = D.CODG_MUSICA
INNER JOIN TBL_CANTAI_CATEGORIA E ON D.CODG_CATEGORIA = E.ID_CATEGORIA

INNER JOIN TBL_CANTAI_CATEGORIA_ALBUM F ON A.ID_ALBUM = F.CODG_ALBUM
INNER JOIN TBL_CANTAI_CATEGORIA G ON F.CODG_CATEGORIA = G.ID_CATEGORIA

WHERE A.NOME LIKE CONCAT("%", 'Amor Esponsal', "%") OR 
A.DESCRICAO LIKE CONCAT("%", 'Amor Esponsal', "%") OR
C.NOME LIKE CONCAT("%", 'Amor Esponsal', "%") OR
C.DESCRICAO LIKE CONCAT("%", 'Amor Esponsal', "%") OR
E.NOME LIKE CONCAT("%", 'Amor Esponsal', "%") OR
G.NOME LIKE CONCAT("%", 'Amor Esponsal', "%")